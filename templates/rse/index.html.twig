					{% set delta12 = ((phytoBiocontroleGloFous[0].montantN1 - phytoBiocontroleGloFous[0].montantN2) / phytoBiocontroleGloFous[0].montantN2)* 100 %}
					{% set colorDelta12 = 'warning' %}
					{% set caretDelta12 = 'left' %}
					{% if delta12 > 0 %}
						{% set colorDelta12 = 'success' %}
						{% set caretDelta12 = 'up' %}
					{% endif %}
					{% if delta12 < 0 %}
						{% set colorDelta12 = 'danger' %}
						{% set caretDelta12 = 'down' %}
					{% endif %}
					<span class="description-percentage text-{{colorDelta12}}">
						<i class="fas fa-caret-{{caretDelta12}}"></i>
						{{ delta12|number_format(2, '.', ' ') }}%
					</span>
					<h5 class="description-header">{{phytoBiocontroleGloFous[0].montantN1|format_currency('EUR', locale='fr') }}</h5>
					<span class="description-text">du
						{{ dd|date("d-m-") }}{{ dd|date("Y") -1 }}
						au
						{{ df|date("d-m-") }}{{ df|date("Y") -1 }}</span>
				</div>
				<!-- /.description-block -->
			</div>
			<!-- /.col -->
			<div class="col-sm-2 col-12">
				<div class="description-block">
					{% set delta01 = ((phytoBiocontroleGloFous[0].montantN - phytoBiocontroleGloFous[0].montantN1) / phytoBiocontroleGloFous[0].montantN1)* 100 %}
					{% set colorDelta01 = 'warning' %}
					{% set caretDelta01 = 'left' %}
					{% if delta01 > 0 %}
						{% set colorDelta01 = 'success' %}
						{% set caretDelta01 = 'up' %}
					{% endif %}
					{% if delta01 < 0 %}
						{% set colorDelta01 = 'danger' %}
						{% set caretDelta01 = 'down' %}
					{% endif %}
					<span class="description-percentage text-{{colorDelta01}}">
						<i class="fas fa-caret-{{caretDelta01}}"></i>
						{{ delta01|number_format(2, '.', ' ') }}%
					</span>
					<h5 class="description-header">{{phytoBiocontroleGloFous[0].montantN|format_currency('EUR', locale='fr') }}</h5>
					<span class="description-text">du
						{{ dd|date("d-m-Y") }}
						au
						{{ df|date("d-m-Y") }}</span>
				</div>
				<!-- /.description-block -->
			</div>
			<!-- /.col -->
		</div>
		<div class="card">
			<div class="card-header border-transparent">
				<div>
					<h2 class="card-title">Détail Phyto et Biocontrôle pour les Fournisseurs</h2>
					<p class="card-text">
						<small>Objectif : Priviléger/Augmenter les produits Biocontrôle</small>
					</p>
				</div>
				<div class="card-tools">
					<button type="button" class="btn btn-tool" data-card-widget="collapse">
						<i class="fas fa-minus"></i>
					</button>
					<button type="button" class="btn btn-tool" data-card-widget="remove">
						<i class="fas fa-times"></i>
					</button>
				</div>
			</div>
			<div class="card-body">
				<div class="table-responsive">
					<table id='example2' class="table table-sm">
						<thead>
							<tr>
								<th>Famille</th>
								<th>TVA</th>
								<th>du
									{{ dd|date("d-m-") }}{{ dd|date("Y") -3 }}
									au
									{{ df|date("d-m-") }}{{ df|date("Y") -3 }}</th>
								<th>du
									{{ dd|date("d-m-") }}{{ dd|date("Y") -2 }}
									au
									{{ df|date("d-m-") }}{{ df|date("Y") -2 }}</th>
								<th>du
									{{ dd|date("d-m-") }}{{ dd|date("Y") -1 }}
									au
									{{ df|date("d-m-") }}{{ df|date("Y") -1 }}</th>
								<th>du
									{{ dd|date("d-m-") }}{{ dd|date("Y") }}
									au
									{{ df|date("d-m-") }}{{ df|date("Y") }}</th>
							</tr>
						</thead>
						<tbody>
							{% for phytoBiocontroleFamFou in phytoBiocontroleFamFous %}
								<tr>
									<td>{{ phytoBiocontroleFamFou.famille }}</td>
									<td>
										{% if phytoBiocontroleFamFou.tva == 1 %}
											20 %
										{% elseif phytoBiocontroleFamFou.tva == 2 %}
											10 %
										{% elseif phytoBiocontroleFamFou.tva == 7 %}
											5.5 %
										{% endif %}
									</td>
									<td>{{ phytoBiocontroleFamFou.montantN3 }}
