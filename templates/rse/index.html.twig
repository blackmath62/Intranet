						<h1>States RSE du
							{{ dd }}
							au
							{{ df }}
						</h1>
					</div>
					{{ form_start(form) }}
					<div class='d-flex'>
						<div class="p-1 mt-2">
							Veuillez selectionner la période :
						</div>
						<div class="p-1">
							{{ form_widget(form.startDate) }}
						</div>
						<div class="p-1">
							{{ form_widget(form.endDate) }}
						</div>
						<div class="p-1">
							{{ form_row(form.filtrer, { 'attr' : { 'class' : 'btn btn-xl btn-secondary' }}
							) }}
						</div>
					</div>
					{{ form_end(form) }}
				</div>
			</div>
