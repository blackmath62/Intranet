											{% set colorDeltaFamilleClientN3 = 'success' %}
										{% elseif deltaFamilleClientN3 == 0  %}
											{% set colorDeltaFamilleClientN3 = 'warning' %}
										{% elseif deltaFamilleClientN3 < 0  %}
											{% set colorDeltaFamilleClientN3 = 'danger' %}
										{% endif %}
									{% else %}
										{% set colorDeltaFamilleClientN3 = 'warning' %}
										{% set deltaFamilleClientN3 = 0 %}
									{% endif %}
