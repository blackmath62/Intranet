										<th class="table-success">{{ "now"|date("Y") - 3 }}</th>
										<th class="table-success">% CA</th>
										<th class="table-danger">{{ "now"|date("Y") - 2 }}</th>
										<th class="table-danger">% CA</th>
										<th class="table-warning">{{ "now"|date("Y") - 1 }}</th>
										<th class="table-warning">% CA</th>
										*</tr>
								</thead>
							</th>
						</tr>
