{% extends 'base.html.twig' %}

{% block title %}
	{{title}}
{% endblock %}

{% block body %}

	{% if app.request.attributes.get('dos') == 1 %}
		{% set commercial = 'app_states_commercial_dd_dfLh' %}
	{% elseif app.request.attributes.get('dos') == 3 %}
		{% set commercial = 'app_states_commercial_dd_df' %}
	{% endif %}

	<div class="row">
		<div class="col-md-12">
			<div class="card card-dark">
				<div class="card-header d-flex bd-highlight">
					<div class="p-2 flex-grow-1 bd-highlight">
						<h3 class="card-title text-center">States
							{{ app.request.attributes.get('metier') }}
							par Commerciaux de
							{{ app.request.attributes.get('dd')|date("d-m-Y") }}
							au
							{{ app.request.attributes.get('df')|date("d-m-Y") }}
						</h3>
					</div>
					<div class="p-2 bd-highlight">
						{{ form_start(form) }}
						<div class='d-flex'>
							<div class="p-1 mt-2">
								Veuillez selectionner la période :
							</div>
							<div class="p-1">
								{{ form_widget(form.startDate) }}
							</div>
							<div class="p-1">
								{{ form_widget(form.endDate) }}
							</div>
							<div class="p-1">
								{{ form_row(form.filtrer, { 'attr' : { 'class' : 'btn btn-xl btn-secondary' }}
								) }}
							</div>
							<div class="p-1">
								<a class="btn btn-success" href="{{path('app_resume_states_dd_df', {'dos' : app.request.attributes.get('dos'),'metier' : app.request.attributes.get('metier'),'dd' : dd, 'df' : df})}}">Résumé</a>
							</div>
						</div>
						{{ form_end(form) }}
					</div>
				</div>
