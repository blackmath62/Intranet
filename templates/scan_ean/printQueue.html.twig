{% if content|length > 0 %}
	<table class="table table-responsive">
		{% for line in content %}
			<tr>
				{% for cell in line %}
					<td>
						{{ cell|raw}}
					</td>
				{% endfor %}
			</tr>
		{% endfor %}
	</table>
{% else %}
	<div class="alert alert-success">
		<b>Aucun blocage détecté, vous pouvez continuer vos impressions.</b>
	</div>
{% endif %}
